{
  "parser": "babel-eslint",
  "extends": "eslint-config-airbnb",
  "rules": {
    "react/jsx-indent": [
      "error",
      4,
      {
        "checkAttributes": true, 
        "indentLogicalExpressions": true
      }
    ],
    "react/jsx-indent-props": [
      "error",
      4
    ],
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": [
      2,
      {
        "ignore": [
          "electron"
        ]
      }
    ],
    "linebreak-style": 0,
    "react/prefer-stateless-function": 0,
    "indent": [
      "error",
      4,
      {"SwitchCase": 1}
    ],
  }
}